<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="regis.model.Votesystem" table="VOTESYSTEM">
<!--      <cache usage="read-write"/>  -->
        <id name="no" column="PL_NO" type="java.lang.Long">
            <generator class="native" />
        </id>
        <property name="coName"  column='CO_NAME' />
        <property name="plClass" column='PL_CLASS' />
        <property name="plName" column='PL_NAME'/>
        <property name="score" column='SCORE' />
        <property name="comment" column='COMMENT'/>
    </class>
</hibernate-mapping>
